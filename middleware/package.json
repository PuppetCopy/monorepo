{
  "name": "@puppet-copy/middleware",
  "license": "BSL-1.1 and MIT",
  "description": "Copytrading middleware",
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "build": "bun run build:clean && bun run build:esm",
    "build:esm": "bunx tsc -b",
    "build:clean": "rm -rf dist",
    "dev:publish": "bun run script/publish.ts"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@gmx-io/sdk": "1.1.2",
    "@most/core": "1.6.1",
    "@most/disposable": "1.3.0",
    "@most/prelude": "1.8.0",
    "@most/scheduler": "1.3.0",
    "@most/types": "1.1.0",
    "abitype": "1.0.8",
    "aelea": "1.9.0",
    "jazzicon": "1.5.0",
    "lightweight-charts": "5.0.8",
    "viem": "2.31.6"
  },
  "devDependencies": {
    "@wagmi/cli": "2.3.1"
  },
  "homepage": "https://puppet.tech",
  "keywords": [
    "puppet",
    "puppetCopy",
    "puppet-middleware",
    "copytrading",
    "eth",
    "ethereum",
    "dapp",
    "wallet"
  ],
  "publishConfig": {
    "access": "public"
  }
}