{
  "name": "@puppet-copy/middleware",
  "license": "BSL-1.1 and MIT",
  "description": "Copytrading middleware",
  "version": "0.9.0",
  "type": "module",
  "scripts": {
    "build": "bun run build:clean && bun run bunx tsc -b",
    "build:clean": "rm -rf dist",
    "build:pre-release": "bun run script/pre-release.ts",
    "tsc:check": "bunx tsc --noEmit"
  },
  "devDependencies": {
    "@wagmi/cli": "2.3.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.js"
    }
  },
  "dependencies": {
    "@gmx-io/sdk": "1.2.0",
    "abitype": "1.0.8",
    "aelea": "2.1.2",
    "jazzicon": "1.5.0",
    "lightweight-charts": "5.0.8",
    "viem": "2.33.2"
  },
  "homepage": "https://puppet.tech",
  "keywords": [
    "puppet",
    "puppetCopy",
    "puppet-middleware",
    "copytrading",
    "eth",
    "ethereum",
    "dapp",
    "wallet"
  ]
}
