{
  "name": "@puppet/sdk",
  "license": "BSL-1.1 and MIT",
  "description": "Copytrading SDK",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "bun run build:clean && bun run tsc -b",
    "build:clean": "rm -rf dist",
    "tsc:check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.js"
    }
  },
  "dependencies": {
    "@puppet/contracts": "workspace:*",
    "abitype": "catalog:",
    "aelea": "catalog:",
    "lightweight-charts": "catalog:",
    "viem": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  },
  "homepage": "https://puppet.tech",
  "keywords": [
    "puppet",
    "puppetCopy",
    "puppet-sdk",
    "copytrading",
    "eth",
    "ethereum",
    "dapp",
    "wallet"
  ]
}