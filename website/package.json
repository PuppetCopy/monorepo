    "@most/types": "^1.1.0",
    "@puppet/middleware": "workspace:*",
    "@ponder/client": "0.10.19",
    "@reown/appkit": "^1.7.3",
    "@reown/appkit-adapter-wagmi": "^1.7.3",
    "@wagmi/core": "^2.17.0",
    "abitype": "^1.0.8",
    "viem": "^2.28.1",
    "color": "^5.0.0",
    "lightweight-charts": "5.0.6",
    "drizzle-orm": "0.41.0",
