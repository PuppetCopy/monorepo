{
  "name": "website",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "VITE_PWA_DEV=true bun --bun vite",
    "dev:generate-pwa-assets": "bun --bun pwa-assets-generator",
    "build": "bun --bun vite build",
    "clean": "bun run clean:rimraf && tsc -build --clean",
    "clean:rimraf": "rimraf dist && tsc -build --clean",
    "preview": "bun --bun vite preview",
    "build:tsc:all": "bun --filter '*' build:tsc",
    "build:clean:all": "bun --filter '*' clean:tsc",
    "tsc:check": "tsc --noEmit",
    "start": "bun --bun serve dist -s -l $PORT"
  },
  "dependencies": {
    "@puppet/contracts": "workspace:*",
    "@puppet/database": "workspace:*",
    "@puppet/sdk": "workspace:*",
    "@rhinestone/sdk": "1.0.40",
    "@wagmi/connectors": "7.0.2",
    "@wagmi/core": "3.0.0",
    "abitype": "catalog:",
    "aelea": "catalog:",
    "color": "5.0.3",
    "lightweight-charts": "catalog:",
    "mersenne-twister": "1.1.0",
    "porto": "0.2.37",
    "viem": "catalog:",
    "wagmi": "3.1.0"
  },
  "devDependencies": {
    "@ponder/client": "0.15.17",
    "@rollup/plugin-replace": "6.0.3",
    "@types/chrome": "catalog:",
    "@vite-pwa/assets-generator": "1.0.2",
    "dotenv": "catalog:",
    "rimraf": "6.1.2",
    "typescript": "catalog:",
    "vite": "7.3.0",
    "vite-plugin-pwa": "1.2.0",
    "vite-tsconfig-paths": "6.0.3",
    "workbox-core": "7.4.0",
    "workbox-expiration": "7.4.0",
    "workbox-precaching": "7.4.0",
    "workbox-routing": "7.4.0",
    "workbox-strategies": "7.4.0",
    "workbox-window": "7.4.0"
  },
  "resolutions": {
