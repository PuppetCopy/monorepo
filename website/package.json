{
  "name": "client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun vite",
    "dev:schemaFiles": "mkdir -p ./src/__generated__ && cp ../subgraph/ponder.schema.ts ../subgraph/ponder.types.ts ./src/__generated__/",
    "dev:generate-pwa-assets": "pwa-assets-generator",
    "build": "bun vite build",
