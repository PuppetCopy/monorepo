{
  "npm.packageManager": "bun",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    // "**/dist": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/*.code-search": true,
    "**/dist": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "typescript.format.enable": true,
  "npm.scriptRunner": "bun"
}